{
  "name": "@mfellner/react-native-bip39",
  "version": "2.3.0-beta.0",
  "license": "MIT",
  "description": "ReactNative-ready Bitcoin BIP39: Mnemonic code for generating deterministic keys",
  "repository": "http://github.com/mfellner/react-native-bip39",
  "keywords": [
    "bip39",
    "mnemonic",
    "react",
    "native"
  ],
  "author": "Alberto Dallaporta",
  "contributors": [
    "Maximilian Fellner <mail@mfellner.eu>"
  ],
  "module": "dist/src/index.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist/",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "test": "jest",
    "typecheck": "tsc -P tsconfig.json --noEmit",
    "release": "release-it",
    "clean": "rm -rf dist/",
    "build": "tsc -P tsconfig.build.json",
    "build:clean": "yarn clean && yarn build",
    "prepare": "husky install"
  },
  "lint-staged": {
    ".ts": "eslint --fix"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "create-hash": "^1.2.0",
    "react-native-fast-crypto": "^2.2.0",
    "react-native-securerandom": "^1.0.0",
    "unorm": "^1.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@release-it/conventional-changelog": "4.2.2",
    "@types/create-hash": "1.2.2",
    "@types/jest": "27.4.1",
    "@types/unorm": "1.3.28",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "babel-jest": "27.5.1",
    "eslint": "8.12.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-runner-eslint": "1.0.0",
    "lint-staged": "12.3.7",
    "metro-react-native-babel-preset": "0.70.1",
    "react-native": "0.68.0",
    "release-it": "14.14.1",
    "typescript": "4.6.3"
  }
}
