{
  "compilerOptions": {
    "outDir": "dist/",
    "target": "ES2019",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "checkJs": false,
    "resolveJsonModule": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  }
}
