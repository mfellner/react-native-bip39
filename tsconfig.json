{
  "compilerOptions": {
    "outDir": "dist/",
    "target": "es2017",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "sourceMap": true,
    "noEmitOnError": true
  }
}
